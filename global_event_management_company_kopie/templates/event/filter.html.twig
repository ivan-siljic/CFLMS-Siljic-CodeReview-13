 {# {% block body %}
    <h1>Results</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Date</th>
                <th>Description</th>
                <th>Image</th>
                <th>Capacity</th>
                <th>Email</th>
                <th>Phone_number</th>
                <th>City</th>
                <th>Zip</th>
                <th>Street</th>
                <th>House_number</th>
                <th>Url</th>
                <th>Type</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for result in results %}
            <tr>
                <td>{{ result.id }}</td>
                <td>{{ result.name }}</td>
                <td>{{ result.date ? result.date|date('Y-m-d') : '' }}</td>
                <td>{{ result.description }}</td>
                <td>{{ result.image }}</td>
                <td>{{ result.capacity }}</td>
                <td>{{ result.email }}</td>
                <td>{{ result.phoneNumber }}</td>
                <td>{{ result.city }}</td>
                <td>{{ result.zip }}</td>
                <td>{{ result.street }}</td>
                <td>{{ result.houseNumber }}</td>
                <td>{{ result.url }}</td>
                <td>{{ result.type }}</td>
                <td>
                    <a href="{{ path('event_show', {'id': result.id}) }}">show</a>
                    <a href="{{ path('event_edit', {'id': result.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="15">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('event_new') }}">Create new</a>
   
{% endblock %} #}